{
  "name": "LiveChart",
  "search_url": "https://www.livechart.me/search?q={query}",
  "selectors": {
    "container": "div.anime-card",
    "title": "h3.main-title > a",
    "episode": "div.poster-overlay > span.ep-number",
    "link": "h3.main-title > a",
    "rating": "div.anime-avg-score"
  },
  "infinite_scroll": false
}